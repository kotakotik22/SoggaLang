fun boolean isOfType type any {
    pizzer typeOfArg getting typeof args.1
    pizzer equalsToType getting equals args.0, typeOfArg
    return equalsToType
}

fun boolean isString any {
    pizzer varIsString getting isOfType /string/, args.0
    return varIsString
}

fun boolean isInt any {
    pizzer varIsInt getting isOfType /integer/, args.0
    return varIsInt
}

